{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    }
  ],
  "git": {
    "deploymentEnabled": false
  },
  "env": {
    "VERCEL_GITHUB_DISABLED": true
  }
}
